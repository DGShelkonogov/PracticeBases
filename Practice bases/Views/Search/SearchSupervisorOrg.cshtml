@model List<BaseRow>

@{
    ViewData["Title"] = "Search";
    Layout = "_SearchLayout";
}

@{
    if (Model != null)
    {
        <div class="list-group">
            @{
                foreach (var item in Model)
                {
                    <a asp-action="DetailsSupervisorOrg" asp-route-id="@item.Id" class="list-group-item list-group-item-action">
                        <p class="list-group-item-text">Руководитель от техникума: @item.SupervisorOrg.Human.Name</p>
                        <p class="list-group-item-text">Телефон руководителя от техникума: @item.SupervisorOrg.Phone.Title</p>
                    </a>
                }
            }
        </div>
    }
}